@model Login

    <form asp-action="Login" asp-controller="Login" method="post">
  <div class="form-group">
    <label asp-for="User">Username</label>
    <input type="text" class="form-control" asp-for="User" aria-describedby="emailHelp" placeholder="Enter username">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input class="form-control" placeholder="Inser password" asp-for="Password" type="text">
</div>


  <button type="submit" class="btn btn-primary">Submit</button>
</form>
