@model LoginModel

    <form asp-action="Login" asp-controller="Account" method="post">
  <div class="form-group">
    <label asp-for="Email">Email</label>
    <input type="text" class="form-control" asp-for="Email" aria-describedby="emailHelp" placeholder="Enter Email">
  </div>
  <div class="form-group">
    <label>Password</label>
    <input class="form-control" placeholder="Inser password" asp-for="Password" type="text">
</div>


  <button type="submit" class="btn btn-primary">Submit</button>
</form>
